{
  "name": "kukulkan-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.1.0",
  "files": [
    "lib",
    "src",
    "build"
  ],
  "dependencies": {
    "@theia/core": "latest",
    "@theia/languages": "latest",
    "@theia/monaco": "latest"
  },
  "devDependencies": {
    "rimraf": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "backend": "lib/node/kukulkan-backend-module",
      "frontend": "lib/browser/kukulkan-frontend-module"
    }
  ]
}
